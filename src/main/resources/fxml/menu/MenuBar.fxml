<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar
        fx:id="menuBar"
        xmlns="http://javafx.com/javafx/9"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="ui.menu.MenuController"
        stylesheets="@/css/menu_styles.css, @/css/styles.css"
>
    <menus>
        <Menu id="menuSettings" fx:id="menuSettings" mnemonicParsing="false" text="%Menu.Settings">
            <items>
                <MenuItem fx:id="menuItemLanguage" text="%Dialog.Title.Language" />
            </items>
        </Menu>
    </menus>
</MenuBar>
